{
    "build": {
        "commands": [
            "pip install -r requirements.txt"
        ]
    },
    "deploy": {
        "commands": [
            "python manage.py migrate",
            "python manage.py collectstatic --noinput"
        ],
        "start": "gunicorn DidiCameras.wsgi:application --bind 0.0.0.0:5432"
    }
}